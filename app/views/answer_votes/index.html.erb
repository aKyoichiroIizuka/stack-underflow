<h1>Listing answer_votes</h1>

<table>
  <thead>
    <tr>
      <th>value</th>
      <th>user</th>
      <th>answer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_votes.each do |answer_vote| %>
      <tr>
        <td><%= answer_vote.value %></td>
        <td><%= answer_vote.user && answer_vote.user.name %></td>
        <td><%= answer_vote.answer_id %></td>
        <td><%= link_to 'Show', answer_vote %></td>
        <td><%= link_to 'Edit', edit_answer_vote_path(answer_vote) %></td>
        <td><%= link_to 'Destroy', answer_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer vote', new_answer_vote_path %>
